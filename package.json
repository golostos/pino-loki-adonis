{
  "name": "pino-loki-adonis",
  "version": "1.0.1",
  "description": "A transport for pino that sends messages to Loki in AdonisJS 4.1, based on the pino-loki package",
  "author": "Alexander Dmitriev",
  "repository": {
    "type": "git",
    "url": "https://github.com/golostos/pino-loki-adonis.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "commander": "^9.4.0",
    "pino-abstract-transport": "^1.0.0",
    "pump": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "adonis-types": "^1.0.8",
    "husky": "^8.0.3",
    "rimraf": "^4.1.2",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2"
  },
  "scripts": {
    "build": "rimraf dist && unbuild",
    "precommit": "npm version patch --no-git-tag-version && git add package.json",
    "postcommit": "npm publish",
    "prepare": "husky install && npm run build"
  },
  "keywords": [
    "Pino",
    "Loki",
    "AdonisJS"
  ],
  "license": "Unlicense",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ]
}
